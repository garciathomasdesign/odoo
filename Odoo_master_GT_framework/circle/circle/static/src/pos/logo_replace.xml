<?xml version="1.0" encoding="utf-8" ?>
<!--This opration is inherited pos system and replaced the logo provided by company -->
<template id="template" xml:space="preserve">
    <t t-inherit="point_of_sale.Chrome" t-inherit-mode="extension" owl="1">
        <xpath expr="//img[hasclass('pos-logo')]" position="attributes" >
<!--            replacer can contain any .jpeg,jpg,png files -->
            <attribute name="src">/circle/static/src/brand_identity/logo.jpg</attribute>
        </xpath>
    </t>
</template>